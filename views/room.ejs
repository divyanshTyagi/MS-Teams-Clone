<body>
    
   <div class = "app-header-bar is-in-call">
      <div class = "in-call-bar-container">
          <div class = "in-call-bar">
              <div class = "in-call-search-bar-outer">
                  <form role = "search">
                      <span class = "search-icon"> 
                          <i class="fa fa-search" aria-hidden="true"></i>
                      </span>
                      <input class = "in-call-search-bar-input">

                  </form>
              </div>
          </div>
      </div>
      <div class = "in-call-profile">
          <div class = "in-call-setting-container">
              <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
          </div>
          <div class = "in-call-profile-container">
              <i class="fa fa-user" aria-hidden="true"></i>
          </div>
      </div>
  </div>

  <div class = "main">
      <div class = "app-bar-v-container">
         <div class="app-bar-v">
            <ul class = "app-bar-items">
               <li>
                     <button class = "app-bar-button">
                        <i class="fa fa-bell-o" aria-hidden="true"></i>
                     </button> 
               </li>
               <li>
                     <button class = "app-bar-button">
                        <i class="fa fa-commenting-o" aria-hidden="true"></i>
                     </button> 
               </li>
               <li>
                     <button class = "app-bar-button">
                        <i class="fa fa-calendar" aria-hidden="true"></i>
                     </button> 
               </li>
               <li role = "presentation" class = "spacer"></li>
               <li>
                     <button class = "app-bar-button">
                        <i class="fa fa-question-circle" aria-hidden="true"></i>
                     </button> 
               </li>
               <li>
                     <button class = "app-bar-button">
                        <i class="fa fa-download" aria-hidden="true"></i>

                     </button> 
               </li>
            </ul>
         </div>

   </div>

        
      <div class="main__videos">

            <div id="video-grid">  </div>

            <div class="main__controls">
               <div class="main__controls__button main__video_button" onClick = "playStop()">
                  <i class="fas fa-video-slash"></i>
               </div>
               <div class="main__controls__button main__mute_button" onClick="muteUnmute()">
                  <i class="fas fa-microphone"></i>
               </div>
               <div class="main__controls__button">
                  <i class="fas fa-microphone"></i>
               </div>
               <div class="main__controls__button">
                  <i class="fas fa-ellipsis-h"></i>
               </div>
               <div class="main__controls__button">
                  <i class="fas fa-hand-spock"></i>
               </div>
               <div class="main__controls__button">
                  <i class="fas fa-comment-dots"></i>
               </div>
               <div class="main__controls__button">
                  <i class="fas fa-users"></i>
               </div>
               <div class="main__controls__button" onClick="leaveCall()">
                  <i class="fas fa-phone-slash"></i>
               </div>
               
